{
  "name": "node-pluginsmanager-plugin-tradfri",
  "version": "1.0.0",
  "description": "A plugin to manage tradfri and execute command lines for node-pluginsmanager",
  "main": "lib/main.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "npx eslint ./lib/**/*.js ./test/**/*.js",
    "check-updates": "npx check-version-modules",
    "unit-tests": "npx nyc --reporter=html --reporter=text mocha",
    "tests": "npm run-script lint && npm run-script check-updates && npm run-script unit-tests",
    "ci": "npm run-script tests && npx nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "/lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script lint",
      "pre-push": "npm run-script tests"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Psychopoulet/node-pluginsmanager-plugin-tradfri"
  },
  "keywords": [
    "tradfri",
    "trådfri",
    "lights",
    "remote controls"
  ],
  "author": "Sébastien VIDAL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Psychopoulet/node-pluginsmanager-plugin-tradfri/issues"
  },
  "dependencies": {
    "node-pluginsmanager-plugin": "~3.1.2",
    "node-tradfri-client": "~2.0.1"
  },
  "devDependencies": {
    "@types/node": "12.12.0",
    "@types/socket.io": "2.1.4",
    "@types/ws": "6.0.3",
    "check-version-modules": "1.3.0",
    "coveralls": "3.0.7",
    "eslint": "6.6.0",
    "express": "4.17.1",
    "husky": "3.0.9",
    "mocha": "6.2.2",
    "node-pluginsmanager": "2.1.7",
    "nyc": "14.1.1",
    "ws": "7.2.0"
  },
  "homepage": "https://github.com/Psychopoulet/node-pluginsmanager-plugin-tradfri#readme",
  "engines": {
    "node": ">=6.0.0"
  }
}
