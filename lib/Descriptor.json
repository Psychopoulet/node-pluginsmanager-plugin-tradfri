							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			},
			"put": {
				"description": "",
				"summary": "Set the gateway IP",
				"operationId": "setIP",
				"responses": {
					"201": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"type": "void"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/node-pluginsmanager-plugin-tradfri/api/authenticate": {
			"put": {
				"description": "",
				"summary": "Generate the gateway conf",
				"operationId": "setIP",
				"responses": {
					"201": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"type": "void"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
