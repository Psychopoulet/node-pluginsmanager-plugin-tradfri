			"get": {
				"description": "",
				"summary": "Get the gateway IP",
				"operationId": "getIP",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"type": "string"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
