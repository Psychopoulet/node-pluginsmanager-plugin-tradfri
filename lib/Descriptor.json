				"description": "Structure for Device",
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					},
					"type": {
						"type": "string"
					},
					"alive": {
						"type": "boolean"
					},
					"createdAt": {
						"type": "string"
					},
					"lastSeen": {
						"type": "string"
					}
				},
				"required": [
					"id", "name", "type", "alive", "createdAt", "lastSeen"
				]
			},
			"Error": {
				"description": "Structure for Error",
				"type": "object",
				"properties": {
					"code": {
						"type": "string"
					},
					"message": {
						"type": "string"
					}
				},
				"required": [
					"code",
					"message"
				]
			}
		}
	},
	"servers": [
