			"get": {
				"description": "",
				"summary": "Return parsed descriptor",
				"responses": {
					"200": {
						"description": "Returned descriptor",
						"content": {
							"application/json": {
								"schema": {
									"type": "object"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/node-pluginsmanager-plugin-tradfri/api/devices": {
			"get": {
				"description": "",
				"summary": "Return all running tradfri",
				"operationId": "getAllDevices",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/Device"
									}
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
